/* Preprocessor definitions */

.m --: 1 - ;
.m rot: q< swap q> swap ;
.m choose: &swap ? drop ;
.m branch: choose eval ;
.m ifte: rot branch ;

.m fact: dup [ dup -- fact * ] [ drop 1 ] ifte ;

/* Main program */
.m 10 fact -1 *

.m 100 fact -1 *

.

