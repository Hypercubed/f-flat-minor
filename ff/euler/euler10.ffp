/* http://projecteuler.net/index.php?section=problems&id=10 */

simple?: [ even? ] [ 3 divisor? ] bi or ;

_prime_loop: dup2 [ dup * > ] [ % 0 > ] bi2 and [ 2 + _prime_loop ] ? ;
_prime?: 5 _prime_loop dup * < ;

prime?: dup 4 < [ 1 > ] [
    dup simple? [ drop false ] [ _prime? ] branch
  ] branch ;

